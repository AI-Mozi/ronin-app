<% content_for :breadcrumbs  do %>
<nav class="breadcrumb" aria-label="breadcrumbs">
  <ul>
    <li><a href="/">Home</a></li>
    <li><a href="/db">Database</a></li>
    <li><a href="/db/url_query_param_names">URL Query Param Names</a></li>
  </ul>
</nav>
<% end %>

<h1>URL Query Param Name: <%=h @url_query_param_name %></h1>

<table class="table">
  <tbody>
    <tr>
      <td><strong>Name:</strong></td>
      <td><%=h @url_query_param_name.name %></td>
    </tr>

    <tr>
      <td><strong>Values:</strong></td>
      <td>
        <% @url_query_param_name.query_params.each do |query_param| %>
          <p><kbd><%=h query_param.value %></kbd></p>
        <% end %>
      </td>
    </tr>

    <tr>
      <td><strong>URLs:</strong></td>
      <td>
        <% @url_query_param_name.urls.each do |url| %>
          <p>
            <a href="/db/urls/<%=h url.id %>">
              <%=h url %>
            </a>
          </p>
        <% end %>
      </td>
    </tr>

    <tr>
      <td><strong>Created:</strong></td>
      <td><%=h @url_query_param_name.created_at %></td>
    </tr>
  </tbody>
</table>
